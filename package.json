{
  "name": "ngl-clj",
  "version": "0.8.0",
  "description": "A ngl Widget for Common Lisp Jupyter",
  "keywords": [
    "nglview",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
    "{js}/**/*.{js,ts,map}",
    "css/*.css",
    "LICENSE.md"
  ],
  "homepage": "https://github.com/yitzchak/nglview-clj",
  "bugs": {
    "url": "https://github.com/yitzchak/nglview-clj/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Tarn W. Burton",
    "email": "twburton@gmail.com"
  },
  "main": "js/index.js",
  "types": "./js/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/yitzchak/nglview-clj"
  },
  "scripts": {
    "build": "tsc",
    "lab-install": "yarn pack -f ngl-clj.tgz && jupyter-labextension install --no-build ngl-clj.tgz && sleep 5 && jupyter-lab build --dev-build=True",
    "lint": "eslint . --ext .ts --fix",
    "lint-check": "eslint . --ext .ts",
    "prepack": "yarn run build"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^4.0.0",
    "case": "^1.6.3",
    "ngl": "2.0.0-dev.39"
  },
  "devDependencies": {
    "@phosphor/application": "^1.7.3",
    "@phosphor/widgets": "^1.9.3",
    "@types/node": "^15.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "lint-staged": "^11.0.1",
    "typescript": "^4.1.3"
  },
  "jupyterlab": {
    "extension": "js/plugin"
  },
  "lint-staged": {
    "*.ts": [
      "eslint . --ext .ts --fix"
    ]
  },
  "prettier": {
    "singleQuote": true
  }
}
